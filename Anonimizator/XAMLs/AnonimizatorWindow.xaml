<Window x:Class="Anonimizator.AnonimizatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anonimizator"
        xmlns:xamLs="clr-namespace:Anonimizator.XAMLs"
        mc:Ignorable="d"
        Title="Anonimizator"
        DataContext="{Binding AnonimizatorViewModel, Source={StaticResource Locator}}"
        Width="820">
    <TabControl HorizontalAlignment="Left" Height="Auto" Width="800">
        <TabItem Header="K-anonimizacja">
            <Grid>
                <local:KAnonimization />
            </Grid>
        </TabItem>
        <TabItem Header="Anonimizacja">
            <local:Anonimization />
        </TabItem>
        <TabItem Header="PID K-Anonimizacja">
            <xamLs:PIDKAnonimization />
        </TabItem>
        <TabItem Header="Robocze">
            <local:WorkPanel />
        </TabItem>
        
    </TabControl>
</Window>
